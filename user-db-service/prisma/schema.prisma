generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "windows"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  UserId      String  @id
  Name        String?
  Email       String?
  Password    String?
  Role        String?
  GoogleId    String?
  LoginType   String?
}
